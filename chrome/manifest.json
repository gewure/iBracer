{
  "manifest_version": 2,

  "name": "Österreich Plugin",
  "title":"Österreich Plugin",
  "description": "Ersetzt bestimmte Strings mit anderen bestimmten Strings. Satireprojekt.",
  "version": "1.1",
  "author":"gewure & aaad",

  "browser_action": {
    "default_icon": "afa_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ],
   "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["source.js"],
      "run_at": "document_end"
    }
  ]
}
