{
  "manifest_version": 2,

  "name": "Pirate Hook's Banana Extension",
  "description": "substitutes some text :)",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
    "tabs"
  ],
   "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["source.js"],
      "run_at": ["document_end"]
    }
  ]
}
